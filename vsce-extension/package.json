{
	"name": "art2ascii",
	"displayName": "art2ascii",
	"description": "VSCode extension for art2ascii to display your ascii art!",
	"version": "1.0.3",
	"publisher": "thaeriem",
	"engines": {
		"vscode": "^1.74.0"
	},
	"author": "Thaeriem",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Thaeriem/art2ascii.git"
	},
	"categories": [
		"Other"
	],
	"icon": "assets/icon.png",
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "custom-activitybar",
					"title": "art2ascii",
					"icon": "assets/logo_bito.svg"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "art2ascii.upload-art",
					"when": "view == art2ascii.artView",
					"group": "navigation@1"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "art2ascii.artView",
					"name": "art2ascii"
				}
			]
		},
		"commands": [
			{
				"command": "art2ascii.artView.focus",
				"category": "art2ascii",
				"title": "Open Sidebar",
				"icon": "$(clear-all)"
			},
			{
				"command": "art2ascii.upload-art",
				"title": "Upload Image/Gif",
				"category": "art2ascii",
				"icon": {
					"dark": "assets/icon_light.svg",
					"light": "assets/icon_dark.svg"
				}
			},
			{
				"command": "art2ascii.render",
				"category": "art2ascii",
				"title": "Render ASCII"
			}
		],
		"configuration": [
			{
				"properties": {
					"art2ascii.gifPath": {
						"type": "string",
						"default": ""
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@electron/rebuild": "^3.6.0",
		"@types/node": "^18",
		"@types/vscode": "^1.73.0",
		"@types/punycode": "^2.1.4",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vscode/test-electron": "^2.3.9",
		"eslint": "^8.26.0",
		"source-map-support": "^0.5.12",
		"typescript": "^5.4.2"
	},
	"dependencies": {
		"ansi_up": "^5.2.1",
		"gif-frames": "^1.0.1",
		"heap-js": "^2.5.0",
		"sharp": "^0.33.4"
	}
}
