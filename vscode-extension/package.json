{
	"name": "art2ascii",
	"displayName": "art2ascii",
	"description": "VSCode extension for art2ascii to display your ascii art!",
	"version": "0.0.1",
	"publisher": "Thaeriem",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "custom-activitybar",
					"title": "art2ascii",
					"icon": "assets/logo_bito.svg"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "art2ascii.upload-art",
					"when": "view == art2ascii.artView",
					"group": "navigation@1"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "art2ascii.artView",
					"name": "art2ascii"
				}
			]
		},
		"commands": [
			{
				"command": "art2ascii.artView.focus",
				"category": "art2ascii",
				"title": "Open Sidebar",
				"icon": "$(clear-all)"
			},
			{
				"command": "art2ascii.upload-art",
				"title": "Upload Image/Gif",
				"category": "art2ascii",
				"icon": {
					"dark": "assets/icon_light.svg",
					"light": "assets/icon_dark.svg"
				}
			},
			{
				"command": "art2ascii.terminal",
				"title": "Render Art",
				"category": "art2ascii"
			}
		],
		"configuration": [
			{
				"properties": {
					"art2ascii.gifPath": {
						"type": "string",
						"default": ""
					},
					"art2ascii.gifUri": {
						"type": "string",
						"default": ""
					},
					"art2ascii.reload": {
						"type": "number"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@electron/rebuild": "^3.6.0",
		"@types/node": "^18",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vscode/test-electron": "^2.3.9",
		"electron-rebuild": "^3.2.9",
		"eslint": "^8.26.0",
		"source-map-support": "^0.5.12",
		"typescript": "^5.4.2"
	},
	"dependencies": {
		"ansi_up": "^5.2.1"
	}
}
